{
  "name": "@rimtrans/ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean:dist": "rimraf dist",
    "gen-files": "yarn ts scripts/gen-files",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --report --report-json",
    "lint:scss": "stylelint src/**/*.scss",
    "lint:ts": "tslint -p tsconfig.json",
    "lint": "yarn lint:scss && yarn lint:ts",
    "ts": "ts-node -r tsconfig-paths/register -P ../../../scripts/tsconfig.json"
  },
  "dependencies": {
    "@huiji/vue-locale": "^1.2.0",
    "@rimtrans/service": "^0.1.0",
    "@rimtrans/worker": "^0.1.0",
    "void-ui": "^0.7.21",
    "vue": "^2.6.5",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.3.0",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-typescript": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "fibers": "^3.1.1",
    "sass": "^1.16.1",
    "sass-loader": "^7.1.0",
    "typescript": "^3.2.4",
    "vue-template-compiler": "^2.6.5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
